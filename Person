public class Person
{
    private Name name;
    private int height;

    // Конструктор
    public Person(Name name, int height) {
        this.name = name;
        this.height = height;
    }

    // Геттеры
    public Name getName() {
        return name;
    }

    public int getHeight() {
        return height;
    }

    // Сеттеры
    public void setName(Name name) {
        this.name = name;
    }

    public void setHeight(int height) {
        this.height = height;
    }

    // Возвращение строки
    @Override
    public String toString() {
        return name.toString() + ", рост: " + height;
    }

}
